    "babel-jest": "^17.0.2",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.3.14",
    "babel-preset-react-app": "0.2.1",
    "backbone": "1.2.3",
    "backbone.marionette": "2.4.3",
    "blueimp-md5": "1.1.1",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "classnames": "2.2.0",
    "clipboard": "1.5.5",
